   "source": [
    "# Special Pythagorean Triplet\n",
    "\n",
    "find $abc$ that solves \n",
    "\n",
    "$a+b+c=1000, a^2+b^2=c^2$"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "0\n",
      "31875000\n",
      "31875000\n",
      "0\n"
     ]
    }
   ],
   "source": [
    "for a in range(1000):\n",
    "    for b in range(1000):\n",
    "        if a**2+b**2 == (1000-a-b)**2:\n",
    "            print(a*b*(1000-a-b))\n"
   ]
  }
