   "source": [
    "# Power Digit Sum\n",
    "\n",
    "What is the sum of the digits of the number $2^{1000}$"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "1366\n"
     ]
    }
   ],
   "source": [
    "\n",
    "s = str(2**1000)\n",
    "sm = 0\n",
    "for c in s:\n",
    "    sm += int(c)\n",
    "print(sm)"
   ]
  }
